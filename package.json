{
  "name": "react-giphy",
  "version": "0.0.1",
  "description": "A React component displaying awesome gifs from Giphy.com.",
  "main": "index.js",
  "scripts": {
    "serve": "http-server 'demo/'",
    "start": "http-server 'demo/'",
    "build": "mkdir -p demo/build/ && browserify -t reactify demo/index.js -o demo/build/app.js",
    "watch": "npm run build && watchify -t reactify demo/index.js -o demo/build/app.js -v",
    "test": "jsxhint --reporter node_modules/jshint-stylish/stylish.js '*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekonstantinidis/react-giphy.git"
  },
  "keywords": [
    "gif",
    "giphy",
    "gifs"
  ],
  "author": "Emmanouil Konstantinidis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ekonstantinidis/react-giphy/issues"
  },
  "homepage": "https://github.com/ekonstantinidis/react-giphy",
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "browserify": "^10.1.2",
    "jshint-stylish": "^1.0.1",
    "jsxhint": "^0.14.0",
    "reactify": "^1.1.0",
    "superagent": "^1.2.0",
    "watchify": "^3.2.1"
  }
}
